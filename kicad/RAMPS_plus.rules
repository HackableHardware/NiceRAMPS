
(rules PCB RAMPS_plus
  (snap_angle 
    none
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 1)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.8)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.5)
    )
  )
  (rule
    (width 152.4)
    (clear 203.4)
    (clear 0.0 (type smd_to_turn_gap))
    (clear 50.8 (type smd_smd))
  )
  (padstack "Via[0-1]_700:300_um"
    (shape
      (circle F.Cu 700.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 700.0 0.0 0.0)
    )
    (attach off)
  )
  (padstack "Via[0-1]_1000:500_um"
    (shape
      (circle F.Cu 1000.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 1000.0 0.0 0.0)
    )
    (attach off)
  )
  (padstack "Via[0-1]_1000:300_um"
    (shape
      (circle F.Cu 1000.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 1000.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_1000:300_um" "Via[0-1]_1000:300_um" default
  )
  (via 
    "Via[0-1]_1000:300_um-kicad_default" "Via[0-1]_1000:300_um" "kicad_default"
  )
  (via 
    "Via[0-1]_1000:500_um" "Via[0-1]_1000:500_um" default
  )
  (via 
    "Via[0-1]_1000:500_um-kicad_default" "Via[0-1]_1000:500_um" "kicad_default"
  )
  (via 
    "Via[0-1]_700:300_um" "Via[0-1]_700:300_um" default
  )
  (via 
    "Via[0-1]_700:300_um-kicad_default" "Via[0-1]_700:300_um" "kicad_default"
  )
  (via_rule
    default "Via[0-1]_700:300_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_700:300_um-kicad_default"
  )
  (via_rule
    0.5 "Via[0-1]_1000:500_um-kicad_default"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 152.4)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    +12V +5V A0 A1 A13 A14 A15 A2
    A6 A7 A8 D10 D13 D24 D26 D28
    D30 D34 D36 D38 D46 D48 D8 D9
